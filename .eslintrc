{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "no-console": "off",
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "double"
      ],
      "semi": [
          "error",
          "always"
      ],
      "guard-for-in": 0,
      "prefer-promise-reject-errors": "off",
  "no-invalid-this": 0
}
}

